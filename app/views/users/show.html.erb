<h1>Estas son las canciones de <%= @user.name %></h1>

<% @user.user_songs.each do |user_song| %>
  <%= user_song.song.name %>
  <%= user_song.song.duration %>
  <% if can? :delete, user_song %>
    <%= link_to 'Remove', remove_song_song_path(user_song.song), method: :delete %>
  <% end %>
<% end %>
